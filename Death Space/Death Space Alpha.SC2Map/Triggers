        <Element Type="FunctionCall" Id="4E7B54AE">
            <FunctionDef Type="FunctionDef" Library="Ntve" Id="00000123"/>
            <ScriptCode>
                lv_targetUnit = libWASD_gv_controlledUnits[lp_player];
                while (libWASD_gv_wASDState[lp_player] != 0x0000) {
                    UnitIssueOrder(
                        lv_targetUnit, 
                        OrderTargetingPoint(
                            AbilityCommand(&quot;move&quot;, 0), 
                            PointWithOffsetPolar(UnitGetPosition(lv_targetUnit), 0.3, libWASD_gf_GetWASDAngle(lp_player) + CameraGetYaw(lp_player))
                            ), 
                        c_orderQueueReplace
                    );
                    Wait(0.0, c_timeGame);
                }
            </ScriptCode>
